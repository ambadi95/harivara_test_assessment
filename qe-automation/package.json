{
  "name": "browserStackCucumber",
  "version": "1.0.0",
  "description": "Webdriver IO with Cucumber",
  "main": "./node_modules/.bin/wdio",
  "scripts": {
    "allure:history": "mkdir -p allure-report/history ./test/reports/allure-results/history && cp -a allure-report/history ./test/reports/allure-results/",
    "allure-generate": "node_modules/.bin/allure generate --clean ./test/reports/allure-results",
    "allure-open": "node_modules/.bin/allure generate --clean ./test/reports/allure-results && allure open",
    "junit-report": "node_modules/.bin/junit-viewer --results=./test/reports/junit-results --save=junit-results.html && open junit-results.html",
    "test-browserstack": "wdio ./test/config/wdio.browserstack.shared.conf.js",
    "test-mobile-android": "wdio test/config/wdio.appium.android.conf.js",
    "test-mobile-ios": "wdio test/config/wdio.appium.ios.conf.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-export-default-from": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@cucumber/cucumber": "^8.0.0-rc.2",
    "@wdio/allure-reporter": "^7.16.3",
    "@wdio/appium-service": "^7.16.3",
    "@wdio/browserstack-service": "^7.16.3",
    "@wdio/cli": "^7.16.3",
    "@wdio/cucumber-framework": "^7.16.3",
    "@wdio/junit-reporter": "^7.16.3",
    "@wdio/local-runner": "^7.16.3",
    "@wdio/selenium-standalone-service": "^7.16.3",
    "@wdio/spec-reporter": "^7.16.3",
    "@wdio/sync": "^7.16.3",
    "allure-commandline": "^2.13.8",
    "appium": "^1.22.0",
    "appium-flutter-finder": "^0.1.0",
    "babel-plugin-source-map-support": "^2.1.3",
    "chai": "^4.3.4",
    "cucumber-tag-expressions": "^2.0.3",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "grunt": "^1.4.1",
    "grunt-cli": "^1.4.3",
    "grunt-cucumberjs": "^2.0.2",
    "grunt-webdriver": "^3.0.0",
    "junit-viewer": "^4.11.1",
    "node-excel-to-json": "0.0.3",
    "selenium-standalone": "^8.0.4",
    "underscore": "^1.13.1",
    "wdio-chromedriver-service": "^7.2.2",
    "wdio-json-reporter": "^3.0.0",
    "yargs": "^17.3.1"
  }
}

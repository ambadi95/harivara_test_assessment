    },
    "merchant": {
      "tags": []
    },
    "network-manager": {
      "tags": []
    },
    "task-manager": {
      "tags": []
    },
    "widgets": {
      "tags": []
    },
    "shared-data-models": {
      "tags": []
    },
    "add_card": {
      "tags": []
    },
    "p2p_payments": {
      "tags": []
    },
    "transactions": {
      "tags": []
    },
    "passcode": {
      "tags": []
    },
    "welcome": {
      "tags": []
    },
    "welcome_business": {
      "tags": []
    }
  },
  "plugins": [
    "@nxrocks/nx-flutter"
  ]
